{
  "name": "remix-with-typescript",
  "version": "5.0.0",
  "private": true,
  "scripts": {
    "build": "remix build",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css && lessc --js ./styles/antd.customise.less app/styles/antd.customise.css",
    "dev": "remix dev",
    "postinstall": "remix setup node",
    "start": "remix-serve build"
  },
  "dependencies": {
    "@remix-run/react": "latest",
    "@remix-run/serve": "latest",
    "antd": "4.22.7",
    "autoprefixer": "10.4.12",
    "less": "4.1.3",
    "postcss-cli": "10.0.0",
    "react": "latest",
    "react-dom": "latest",
    "remix": "latest",
    "tailwindcss": "3.1.8"
  },
  "devDependencies": {
    "@remix-run/dev": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "typescript": "latest"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}